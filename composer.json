{
  "name": "senfenico/senfenico-php",
  "description": "Senfenico payment API sdk",
  "keywords": [
    "senfenico",
    "payment processing",
    "api"
  ],
  "homepage": "https://senfenico.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Senfenico and contributors",
      "homepage": "https://github.com/senfenico/senfenico-php/contributors"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^9.0",
    "friendsofphp/php-cs-fixer": "3.5.0",
    "phpstan/phpstan": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Senfenico\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Senfenico\\": [
        "tests/",
        "tests/Senfenico/"
      ]
    }
  },
  "minimum-stability" : "dev"
}
